  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@vitest/coverage-v8": "^3.2.4",
    "esbuild": "^0.25.8",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.3",
