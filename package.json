{
  "name": "@ioris/core",
  "version": "0.3.4",
  "description": "",
  "type": "module",
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "default": "./dist/index.mjs"
  },
  "directories": {
    "lib": "src",
    "example": "example"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm run build && cd example && npm run dev",
